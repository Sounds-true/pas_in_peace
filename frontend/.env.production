# Production Environment Variables for Vercel
# These will be set in Vercel dashboard

# API Configuration (will be set in Vercel)
NEXT_PUBLIC_API_URL=https://your-backend-api.vercel.app
NEXT_PUBLIC_WS_URL=wss://your-backend-api.vercel.app

# Telegram Bot Configuration
NEXT_PUBLIC_TELEGRAM_BOT_USERNAME=pas_in_peace_bot

# Feature Flags
NEXT_PUBLIC_QUEST_BUILDER_ENABLED=true
NEXT_PUBLIC_ANALYTICS_ENABLED=true
NEXT_PUBLIC_LETTER_MANAGER_ENABLED=true

# Environment
NODE_ENV=production
